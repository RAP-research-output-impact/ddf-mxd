<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://mx.forskningsdatabasen.dk/ns/documents/1.3" targetNamespace="http://mx.forskningsdatabasen.dk/ns/documents/1.3" elementFormDefault="qualified" xml:lang="EN">
  <!--
