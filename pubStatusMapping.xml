<?xml version="1.0"?>
<mapping>

  <description>
    This transformation map is describing the mapping between document
    publication status applied in ORBIT and the MXD format.
  </description>

  <rule>
    <in>unpublished</in>
    <out>u</out>
  </rule>
  <rule>
    <in>submitted</in>
    <out>s</out>
  </rule>
  <rule>
    <in>accepted</in>
    <out>a</out>
  </rule>
  <rule>
    <in>published</in>
    <out>p</out>
  </rule>
  <rule>
    <in>epublished</in>
    <out>e</out>
  </rule>
  <rule>
    <in>in-press-is-not-used-in-orbit</in>
    <out>i</out>
  </rule>
</mapping>
